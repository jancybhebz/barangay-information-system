<!DOCTYPE html>
<html lang="en">
<head>
    {% block meta %}
        <meta name="robots" content="noindex, nofollow" />
        <meta http-equiv="X-UA-COMPATIBLE" content="IE=edge" />
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"/>
        <meta http-equiv="X-UA-Compatible" content="IE=7"/>
    {% endblock meta %}
    {% block stylesheets %}
        {% stylesheets
        'bundles/biscms/css/bootstrap.min.css'
        'bundles/biscms/css/font-awesome.min.css'
        'bundles/biscms/css/style.css'
        filter='cssrewrite' %}
        <link href="{{ asset_url }}" rel="stylesheet" />
        {% endstylesheets %}
    {% endblock stylesheets %}
    <title>{% block title %}Barangay Information System CMS{% endblock %}</title>
</head>
<body>
{% block body %}
{% endblock body %}
{% block javascripts %}
    {% javascripts
    'bundles/biscms/js/jquery.js'
    'bundles/biscms/js/bootstrap.min.js'
    'bundles/biscms/js/custom.js'
    %}
    <script src ="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
{% endblock javascripts %}
</body>
</html>